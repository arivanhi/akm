# Konfigurasi Database PostgreSQL
DB_NAME=health_monitoring_db
DB_USER=admin
DB_PASSWORD=your_strong_password_here
DB_HOST=db
DB_PORT=5432

# Kunci Rahasia untuk Sesi Flask (ganti dengan string acak yang panjang)
SECRET_KEY=change_this_to_a_long_random_string_for_production

# Port Serial untuk koneksi ke ESP32
# Kosongkan saat development di Windows (mode simulasi)
# Isi dengan port yang sesuai di Raspberry Pi (contoh: /dev/ttyUSB0)
MQTT_BROKER_HOST=
MQTT_BROKER_PORT=